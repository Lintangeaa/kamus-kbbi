{
  "name": "kamus-kbbi",
  "version": "1.0.0",
  "description": "Indonesian Dictionary (KBBI) Website using Express.js and EJS",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "bun run server.js",
    "watch": "bun --watch server.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop kamus-kbbi",
    "pm2:restart": "pm2 restart kamus-kbbi",
    "pm2:reload": "pm2 reload kamus-kbbi",
    "pm2:delete": "pm2 delete kamus-kbbi",
    "pm2:logs": "pm2 logs kamus-kbbi",
    "pm2:monit": "pm2 monit",
    "pm2:status": "pm2 status",
    "pm2:prod": "pm2 start ecosystem.config.js --env production",
    "pm2:dev": "pm2 start ecosystem.config.js --env development"
  },
  "keywords": ["kbbi", "indonesian", "dictionary", "express", "ejs"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "express-session": "^1.17.3",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
